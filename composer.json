{
    "name": "axalian/achievements",
    "description": "Achievement system for ZF2",
    "license": "MIT",
    "keywords": [
        "achievements",
        "zf2"
    ],
    "homepage": "http://github.com/axalian/achievements",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "authors": [
        {
            "name": "Michel Maas",
            "email": "michel@michelmaas.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "AxalianAchievements": "src/",
            "AxalianAchievementsTest": "tests/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "suggest": {
        "widmogrod/zf2-assetic-module": "Provides an easy way of copying and building this modules assets"
    },
}
